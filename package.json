    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^9.2.0",
    "redux": "^5.0.1",
    "screenfull": "^6.0.2",
    "three": "^0.172.0",
    "three-stdlib": "2.35.12",
    "zustand": "^5.0.3"
